android_app {
    name: "OnePlusDoze",

    defaults: [
        "SettingsLibDefaults",
    ],

    srcs: ["src/**/*.java"],
    certificate: "platform",
    platform_apis: true,
    system_ext_specific: true,
    privileged: true,

    static_libs: [
        "androidx.core_core",
        "androidx.preference_preference",
        "org.lineageos.settings.resources",
    ],

    resource_dirs: ["res"],

    optimize: {
        proguard_flags_files: ["proguard.flags"],
    },

}
